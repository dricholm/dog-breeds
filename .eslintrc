{
  "env": {
    "browser": true,
    "commonjs": true,
    "jest": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "quotes": ["error", "single"],
    "sort-keys": "error"
  }
}
